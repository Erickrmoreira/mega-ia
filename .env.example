# Ambiente
MEGA_IA_ENV=dev

# Autenticacao
MEGA_IA_REQUIRE_API_KEY=false
MEGA_IA_API_KEY=change-me-api-key
MEGA_IA_TRAIN_API_KEY=change-me-train-key
MEGA_IA_MODEL_INFO_API_KEY=change-me-model-info-key
MEGA_IA_METRICS_API_KEY=change-me-metrics-key

# Integridade de artefato
MEGA_IA_REQUIRE_ARTIFACT_HMAC=false
MEGA_IA_ARTIFACT_HMAC_KEY=change-me-hmac-key
MEGA_IA_STRICT_ARTIFACT_TYPES=false
MEGA_IA_ALLOW_PICKLE_FALLBACK=false

# Rate limit
MEGA_IA_RATE_LIMIT_BACKEND=memory
MEGA_IA_RATE_LIMIT_WINDOW_SECONDS=60
MEGA_IA_RATE_LIMIT_MAX_REQUESTS=30
MEGA_IA_RATE_LIMIT_MAX_BUCKETS=50000
MEGA_IA_REQUIRE_REDIS_RATE_LIMIT=false
MEGA_IA_REQUIRE_REDIS_TRAIN_LOCK=false
MEGA_IA_REDIS_URL=redis://localhost:6379/0

# Treino e fila
MEGA_IA_TRAIN_REDIS_PREFIX=mega_ia
MEGA_IA_TRAIN_COOLDOWN_SECONDS=120
MEGA_IA_TRAIN_JOB_TTL_SECONDS=86400
MEGA_IA_TRAIN_JOB_RETRY_COUNT=1
MEGA_IA_TRAIN_JOB_RETRY_BACKOFF_SECONDS=2
MEGA_IA_TRAIN_MAX_QUEUE_SIZE=10
MEGA_IA_TRAIN_RATE_LIMIT_WINDOW_SECONDS=300
MEGA_IA_TRAIN_RATE_LIMIT_MAX_REQUESTS=3

# Limites de geracao
MEGA_IA_MAX_CANDIDATES=20000
MEGA_IA_MAX_REQUEST_COST=600000

# API/UI
MEGA_IA_CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
MEGA_IA_ENABLE_METRICS_ENDPOINT=true
MEGA_IA_TRUST_PROXY_HEADERS=false
MEGA_IA_TRUSTED_PROXY_IPS=

# Gates principais de qualidade
MEGA_IA_MAX_MAE_REGRESSION=0.03
MEGA_IA_MIN_TOP_DECILE_LIFT=1.0
MEGA_IA_FIRST_MODEL_MAX_MAE_FACTOR=1.20
MEGA_IA_FIRST_MODEL_MIN_TOP_DECILE_LIFT=0.50
